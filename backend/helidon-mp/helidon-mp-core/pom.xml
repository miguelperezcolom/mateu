    </properties>

    <dependencies>
        <dependency>
            <groupId>io.helidon.webserver</groupId>
            <artifactId>helidon-webserver</artifactId>
            <version>4.2.6</version>
        </dependency>
        <dependency>
            <groupId>jakarta.annotation</groupId>
